{
	"multipart": [
		{ "when": { "up":    "true"  }, "apply": { "model": "bigtech:block/copper_lightning_cable_connection", "x":   0           } },
		{ "when": { "down":  "true"  }, "apply": { "model": "bigtech:block/copper_lightning_cable_connection", "x": 180           } },
		{ "when": { "north": "true"  }, "apply": { "model": "bigtech:block/copper_lightning_cable_connection", "x":  90, "y":   0 } },
		{ "when": { "east":  "true"  }, "apply": { "model": "bigtech:block/copper_lightning_cable_connection", "x":  90, "y":  90 } },
		{ "when": { "south": "true"  }, "apply": { "model": "bigtech:block/copper_lightning_cable_connection", "x":  90, "y": 180 } },
		{ "when": { "west":  "true"  }, "apply": { "model": "bigtech:block/copper_lightning_cable_connection", "x":  90, "y": 270 } },
		{ "when": { "up":    "false" }, "apply": { "model": "bigtech:block/copper_lightning_cable_center",     "x":   0           } },
		{ "when": { "down":  "false" }, "apply": { "model": "bigtech:block/copper_lightning_cable_center",     "x": 180           } },
		{ "when": { "north": "false" }, "apply": { "model": "bigtech:block/copper_lightning_cable_center",     "x":  90, "y":   0 } },
		{ "when": { "east":  "false" }, "apply": { "model": "bigtech:block/copper_lightning_cable_center",     "x":  90, "y":  90 } },
		{ "when": { "south": "false" }, "apply": { "model": "bigtech:block/copper_lightning_cable_center",     "x":  90, "y": 180 } },
		{ "when": { "west":  "false" }, "apply": { "model": "bigtech:block/copper_lightning_cable_center",     "x":  90, "y": 270 } }
	]
}